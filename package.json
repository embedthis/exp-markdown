{
    "name": "exp-md",
    "description": "Markdown Plugin for Expansive",
    "version": "0.2.4",
    "keywords": [
        "expansive-plugin",
        "marked"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-md.git"
    },
    "engines": {
        "node": ">= 0.4.0"
    },
    "devDependencies": {
        "expansive": ">=0.6.0",
        "pak": ">=0.11.3"
    },
    "pak": {
        "scripts": {
            "postcache": {
                "script": "npm('install -g', 'marked')"
            }
        }
    },
    "ignore": [
        "**/doc/**",
        "**/man/**"
    ]
}
