{
    "name": "exp-md",
    "description": "Markdown Plugin for Expansive",
    "version": "0.2.2",
    "keywords": [
        "expansive-plugin",
        "marked"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-md.git"
    },
    "engines": {
        "node": ">= 0.4.0"
    },
    "scripts": {
        "postcache": {
            "script": "if (!Cmd.locate('marked')) { if (!Cmd.locate('npm')) { throw 'Cannot locate the \"npm\" utility.' } else { run('npm install -g marked')}}"
        }
    },
    "ignore": [
        "**/doc/**",
        "**/man/**"
    ],
    "pak": {
        "version": ">=0.10"
    }
}
