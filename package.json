{
    "name": "exp-md",
    "description": "Markdown Plugin for Expansive",
    "version": "0.2.0",
    "pak": ">=0.10",
    "keywords": [
        "expansive-plugin",
        "marked"
    ],
    "licenses": "GPL",
    "engines": {
        "node": ">= 0.4.0"
    },
    "scripts": {
        "postcache": {
            "script": "Cmd.locate('marked') || run('npm install -g marked')"
        }
    },
    "ignore": [
        "**/doc/**",
        "**/man/**"
    ],
    "install": false
}
